<h1>Listing houses</h1>

<table>

<% @houses.each do |house| %>
  <tr>
    <td>
      <%=link_to house.biaoti,house %>
      <span>
        <%= house.quyu_text %>
      </span>
      <span>
        <%= house.xiaoqu %>
      </span>
      <span>
        <%= "(#{house.cate_text})" %>
      </span>
    </td>
    <td><%= house.huxing%></td>
    <td><%= house.zujin %>元/月</td>
    <td><%= house.updated_at %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to '发布出租信息', new_house_path %>
