<td><%= form_for(@house) do |f| %></td>
      <table>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :chuzu_fangshi,'我是:' %></td>
          <td>
            <% House::CATE_NUM.each do |name,val| %>
                <%= f.radio_button :chuzu_fangshi,val %><%= name %>
            <% end %>
          </td>
        </tr>

        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :chuzu_fangshi,'出租方式:' %></td>
          <td>
            <% House::CHUZU_FANGSHI.each do |name,val| %>
                <%= f.radio_button :chuzu_fangshi,val %><%= name %>
            <% end %>
          </td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :biaoti,'标题:' %></td>
          <td><%= f.text_field :biaoti %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :dizhi,'地址:' %></td>
          <td>
            <%= f.select :quyu_num,House::QUYU_NUM %>

            <%= f.text_field :dizhi %>
          </td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :xiaoqu,'小区:' %></td>
          <td><%= f.text_field :xiaoqu %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= "户型:" %></td>
          <td>
            <%= f.select :woshi,(1..10),selected:2 %>
            <%= f.label :woshi,'室' -%>
            <%= f.select :keting,(1..3) -%>
            <%= f.label :keting,'厅' -%>
            <%= f.select :chufang,(1..3) -%>
            <%= f.label :chufang,'厨' -%>
            <%= f.select :weishengjian,(1..3) -%>
            <%= f.label :weishengjian,'卫' %>
          </td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :leixing,'房屋类型:' %></td>
          <td><%= f.select :leixing,House::LEIXING %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :zhuangxiu,'装修情况:' %></td>
          <td><%= f.select :zhuangxiu,House::ZHUANGXIU %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= "楼层:" %></td>
          <td>
            <%= f.label :zongcengshu,'总' -%>
            <%= f.number_field :zongcengshu %>层
            第<%= f.number_field :louceng %>
            <%= f.label :louceng,'层' %>
          </td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :chaoxiang,'房屋朝向:' %></td>
          <td><%= f.select :chaoxiang,House::CHAOXIANG %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :mianji,'面积:' %></td>
          <td><%= f.number_field :mianji %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :zujin,'租金:' -%></td>
          <td><%= f.number_field :zujin -%>
            <%= f.label :fu_zujin_fangshi,'付租金方式:' -%>
            <%= f.select :fu_zujin_fangshi,House::FU_ZUJIN_FANGSHI -%>
          </td>
        </tr>
        <tr>
          <td></td>
          <td><%= f.label :fangwu_peizhi,'房屋配置:' %></td>
          <td>
          <% House::FANGWU_PEIZHI.each do |p_z| %>
              <%= check_box_tag "house[fangwu_peizhi][]",p_z %><%= p_z %>
          <% end %>
          </td>
        </tr>
        <tr>
          <td></td>
          <td><%= f.label :miaoshu,'其他描述:' %></td>
          <td><%= f.text_area :miaoshu,:cols=>30,:rows=>2 %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :lianxi_dianhu,'联系电话:' %></td>
          <td><%= f.text_field :lianxi_dianhu %></td>
        </tr>
        <tr>
          <td><%= xinghao %></td>
          <td><%= f.label :lianxi_ren,'联系人:' %></td>
          <td><%= f.text_field :lianxi_ren %></td>
        </tr>
        <tr>
          <td colspan="3"><%= f.submit '提交' %></td>
        </tr>
      </table>
  <% end %>
